#include<stdio.h>
int main()
{
	int a,b,c,d,e,p;
	printf("enter the value of a b c d and e");
	scanf("%d %d %d %d %d",&a,&b,&c,&d,&e);
	p=(a+b+c+d+e)/5;
	if(p>=90)
	printf("distinction");
	else if(p>=70)
	printf("first class");
	else if(p>=50)
	printf("second class");
	else if(p>=40)
	printf("pass");
	else if(p<40)
	printf("fail");
	return 0;
	
}
